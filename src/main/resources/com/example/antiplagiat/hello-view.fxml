<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.antiplagiat.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" text="AntiPlagiat app (RU)">
      <font>
         <Font name="System Bold" size="14.0" />
      </font></Label>
   <Label fx:id="welcomeText1" layoutX="30.0" layoutY="30.0" text="Click &quot;Import&quot; to load your text file (*.txt format only)" />
   <Button fx:id="importButton" mnemonicParsing="false" onAction="#onImportButtonClick" text="Import" />
   <Button fx:id="rewriteButton" mnemonicParsing="false" onAction="#onRewriteButtonClick" text="Rewrite file" visible="false" />
   <Label fx:id="checkFileWrite"/>
</VBox>
